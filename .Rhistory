appendSE <- function(x,y){
x$ERRmin[which(x$model == 'EN')] <- x$value[which(x$model == 'EN')] - (y$value * 1.96)
x$ERRmax[which(x$model == 'EN')] <- x$value[which(x$model == 'EN')] + (y$value * 1.96)
#x$ERRmin[which(is.na(x$ERRmin))] <- 0
#x$ERRmax[which(is.na(x$ERRmax))] <- 0
return(x)
}
